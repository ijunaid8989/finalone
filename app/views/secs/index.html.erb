<section class="addUPclass">
	<%= form_for @sec do |f| %>
		<div class="form-group">
			<%= f.label :name, "Class Name :"%>
		  	<%= f.text_field :name , :class => "form-control", :placeholder => "class name" %>
		</div>
		<div class="form-group">
			<%= f.label :level_id, "Level :"%>
	  		<%= f.select :level_id, options_from_collection_for_select(@level, "id", "name") %>
		</div>
		<%= f.submit "Create" , :class => "btn btn-primary" %>
	<% end %>
</section>

<section id="bothend">
	<%= form_for @level do |f| %>
	<div class="form-group">
		<%= f.label :name, "Level Name :"%>
	  	<%= f.text_field :name , :class => "form-control", :placeholder => "level name" %>
	</div>
	<div class="form-group">
	  	<%= fields_for @subs do |u| %>
			<%= u.label :name, "Subjects Name :" %>
		  	<%= u.text_area :name ,  size: "24x6" , :placeholder => "Subjects name" %>
		<% end %>
	</div>
		<%= f.submit "Create" , :class => "btn btn-primary" %>
	<% end %>
</section>